package com.valtech.traning.hibernate;

public class RegAddress {
	
	private String street;
	private String city;
	private String Zip;
	
	
	
	public RegAddress(String street, String city, String zip) {
		super();
		this.street = street;
		this.city = city;
		Zip = zip;
	}
	public RegAddress() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	

}
